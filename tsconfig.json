{
  "compilerOptions": {
    "target": "ESNext",
    "module": "esnext",
    "lib": ["ESNext", "DOM", "DOM.Iterable", "ScriptHost"],
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "classic",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
